    <h1>EMPLOYEES LIST</h1>

    <div class="col s12 m2 container ">
        <table class="table">
            <thead class="thead-dark">
              <tr>
                <th scope="col">ID</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Email</th>
              </tr>
            </thead>
            <tbody>
                <% for(var i=0; i < test.length; i++) { %>
                    <tr>
                      <td><%=test[i].row_to_json.employee_id%></td>
                      <td><%=test[i].row_to_json.first_name%></td>
                      <td><%=test[i].row_to_json.last_name%></td>
                      <td><%=test[i].row_to_json.email%></td>
                    </tr>
                      <% } %>
            </tbody>
          </table>
         
    </div>

    

  <!------SCRIPTS----->
